{
  "MongoDBSettings": {
    "ConnectionString": "mongodb://root:${DB_PASSWORD}@localhost:27017",
    "DatabaseName": "astronautas"
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Issuer": "${JWT_ISSUER}",
    "Audience": "${JWT_AUDIENCE}"
  }
}